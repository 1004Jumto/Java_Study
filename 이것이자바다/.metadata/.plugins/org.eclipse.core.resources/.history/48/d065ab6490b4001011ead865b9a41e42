package ch06;

public class StaticEx {

	public static void main(String[] args) { 
		System.out.println(Television.info);
		System.out.println(Television.sale);
		
		Television.info = "Change";
		System.out.println(Television.info);
		System.out.println(Television.CLASS_NAME);
		
//		Television.CLASS_NAME = "change";	// 컴파일 에러! 값을 변경할 수 없음.
	}

}
