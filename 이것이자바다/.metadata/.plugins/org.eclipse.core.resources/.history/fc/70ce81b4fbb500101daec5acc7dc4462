package ch08;

public class SmartTV implements IRemoteControl, ISearchable {

	@Override
	public void search() {
		// TODO Auto-generated method stub
		System.out.println("search SmartTV");
	}

	@Override
	public void turnOn() {
		// TODO Auto-generated method stub

		System.out.println("turn on SmartTV");

	}

	@Override
	public void turnOff() {
		// TODO Auto-generated method stub
		System.out.println("turn off SmartTV");

	}

}
