package ch06;

public class ClassEx {

	// 필드 선언
	int fieldName;

	// 메소드 선언
	int test() {
		return 0;
	}

	// 생성자
	public ClassEx() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {

		ClassEx ex = new ClassEx(); // 생성자 호출
		Car mine = new Car();
		
		System.out.println(mine.company);
		System.out.println(mine.speed);
		System.out.println(mine.model);
		
	}

}
