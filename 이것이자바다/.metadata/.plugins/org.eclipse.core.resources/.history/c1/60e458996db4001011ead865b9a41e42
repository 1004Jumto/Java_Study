package ch06;

public class Car {

	String company;
	String model;
	String color;
	int maxSpeed;

	int speed;
	int rpm;
	boolean start;

	Car() {
		model = "Renault"; // 초기화
	}

	Car(int fuel) { 	// 생성자는 리턴이 없음 -> int Car(){} 은 안됨
	} 

	Car(String name, String company) {
		this.model = name;
		this.company = company;
	}

//	Body body;
//	Engine engine;
//	Tire tire;
}
