package ch09;

public class Car {
	Tire tire1 = new Tire() {
		@Override
		public void roll() {
			System.out.println("tire1 굴러간다");
		};
	};
	
	public void run() {
		Tire tire2 = new Tire() {
			@Override
			public void roll() {
				System.out.println("tire2 굴러간다");
			}
		};
	}
	
	public void run2(Tire tire) {
		tire.roll();
	}
}
