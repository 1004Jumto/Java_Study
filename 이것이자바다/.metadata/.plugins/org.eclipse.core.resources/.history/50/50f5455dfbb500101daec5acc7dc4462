package ch08;

public interface RemoteControl {
	// 상수 -> public static final
	double PI = 3.14;
	
	// 추상메소드 -> public abstract
	void turnOn();
	
	// default 메소드 -> 일반메소드 public (인터페이스에서는 메소드 구현 x. 오직 선언만)
	default int add(int x, int y) { test(); return x + y; };
	
	// 정적 메소드 -> public static -> static 이라서 인터페이스에서 메소드 구현. 인터페이스로 직접 접근해서 사용
	static int minus(int x, int y) { return x - y; };
	
	// private -> 인터페이스 내부에서만 사용 가능
	private void test() {};
	
	
}
