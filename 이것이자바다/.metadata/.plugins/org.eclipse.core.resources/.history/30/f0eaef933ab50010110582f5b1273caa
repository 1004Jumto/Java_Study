package ch07;

public class SmartPhone extends Phone {
	public boolean wifi;

	public SmartPhone() { 
		System.out.println("Smartphone 생성자");
	}

	public SmartPhone(String model, String color) {
		super(model, color);
		System.out.println("Smartphone 생성자2");
		this.model = model;
		this.color = color;

	}
	
	@Override		// Annotation
//	public void bell() {
//		System.out.println("두번째 벨");
//	}
	
	public void setWifi(boolean wifi) {
		this.wifi = wifi;
		System.out.println("와이파이 상태 확인");
	}

	public void internet() {
		System.out.println("인터넷에 연결");
	}

}
