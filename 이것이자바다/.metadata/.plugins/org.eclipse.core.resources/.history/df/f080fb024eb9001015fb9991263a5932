package ch13.sec02.exam2;

public class GenericEx {

	public static void main(String[] args) {
		Product<Good, String> prod = new Product<Good, String>();

		prod.setKind(new Tv());
		prod.setModel("스마트티비");

		Good good = prod.getKind();
		String model = prod.getModel();
		
		System.out.println(good);
		System.out.println(prod);
		
		prod.setKind(new Car());
		prod.setModel("스마트 카");
		
		good = prod.getKind();
		model = prod.getModel();
		
		System.out.println(good); 
		System.out.println(prod);

	}

}
