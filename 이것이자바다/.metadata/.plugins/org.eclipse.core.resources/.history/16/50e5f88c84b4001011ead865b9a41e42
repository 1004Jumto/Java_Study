package ch06;

public class Television {
	// 정적 필드
	static String company = "My Com";
	static String model = "LCD";
	static String info;
	static String sale;
	
	static {
		info = company  + " : " + model;
//		sale = price * 0.8;		// 컴파일 에러
		
		Television t = new Television();
		sale = t.price * 0.8;
	}
	
	// 인스턴스 필드
	int price = 100;
}
